{
  "name": "pegasus",
  "version": "0.0.11",
  "private": true,
  "scripts": {
    "prod:build": "npm run client:build && npm run server:build",
    "prod:run": "node server/server.js",
    "dev": "concurrently -c blue,yellow \"npm:server:dev\" \"npm:client:dev\"",
    "server:dev": "npm run server:build && concurrently -c blue,green \"npm:server:build:watch\" \"npm:server:run:watch\"",
    "server:run:watch": "npx nodemon --inspect ./dist/server/server.js",
    "server:build:watch": "npx tsc -w -p tsconfig.server.json",
    "server:build": "npm run server:clean && npm run server:build:tsc && npm run server:copy",
    "server:build:tsc": "tsc -p tsconfig.server.json",
    "server:clean": "rm -rf dist/server",
    "server:copy": "cp package.json dist/",
    "client:dev": "sleep 5 && vite",
    "client:build": "run-p type-check build-only",
    "client:watch": "vue-tsc -w --noEmit -p tsconfig.vitest.json --composite false",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "test:unit": "vitest --environment jsdom --root src/",
    "test:e2e": "start-server-and-test preview :4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' :4173 'cypress open --e2e'"
  },
  "dependencies": {
    "@doist/todoist-api-typescript": "^2.1.2",
    "@fullcalendar/core": "^6.0.2",
    "@fullcalendar/daygrid": "^6.0.2",
    "@fullcalendar/icalendar": "^6.0.2",
    "@fullcalendar/interaction": "^6.0.2",
    "@fullcalendar/list": "^6.0.2",
    "@fullcalendar/timegrid": "^6.0.2",
    "@fullcalendar/vue3": "^6.0.2",
    "@headlessui/tailwindcss": "^0.1.2",
    "@headlessui/vue": "^1.7.7",
    "@heroicons/vue": "^2.0.13",
    "axios": "^1.2.0",
    "cheerio": "^1.0.0-rc.12",
    "daisyui": "^2.45.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "ical.js": "^1.5.0",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.40",
    "node-ical": "^0.15.3",
    "pexels": "^1.4.0",
    "pinia": "^2.0.26",
    "rss-parser": "^3.12.0",
    "swagger-ui-express": "^4.6.0",
    "unsplash-js": "^7.0.15",
    "vite-plugin-ssr": "^0.4.54",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6",
    "xmldom": "^0.6.0",
    "xpath": "^0.0.32"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
    "@tailwindcss/typography": "^0.5.8",
    "@types/express": "^4.17.14",
    "@types/jsdom": "^20.0.1",
    "@types/node": "^18.11.9",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.2.4",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "cypress": "^11.2.0",
    "eslint": "^8.22.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.3.0",
    "jsdom": "^20.0.3",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.19",
    "prettier": "^2.7.1",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.2.4",
    "ts-node": "^10.9.1",
    "typescript": "~4.9.4",
    "vite": "^3.2.4",
    "vitest": "^0.25.3",
    "vue-tsc": "^1.0.11"
  }
}

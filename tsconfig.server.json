{
  "compilerOptions": {
    "outDir": "dist/",
    "sourceMap": true,
    "noImplicitAny": false,
    "noUnusedLocals": true,
    "strictNullChecks": true,
    "allowSyntheticDefaultImports": true,
    "allowJs": true,
    "preserveConstEnums": true,
    "target": "es5",
    "module": "CommonJS"
  },
  "include": ["src/server/**/*", "src/common/**/*"]
}
